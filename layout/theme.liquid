<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>


    <title>
      {{ page_title }}
    </title>


	{{content_for_header}}

    {% style %}
      @font-face {
        font-family: "Montserrat-Black";
        src: url({{ "Montserrat-Black.woff" | asset_url }}) format("woff");
      }
      @font-face {
        font-family: "Montserrat-Italic";
        src: url({{ "Montserrat-Italic.woff" | asset_url }}) format("woff");
      }
      @font-face {
        font-family: "Montserrat-Light";
        src: url({{ "Montserrat-Light.woff" | asset_url }}) format("woff");
      }
      @font-face {
        font-family: "Montserrat-Medium";
        src: url({{ "Montserrat-Medium.woff" | asset_url }}) format("woff");
      }
      @font-face {
        font-family: "Montserrat-Bold";
        src: url({{ "Montserrat-Bold.woff" | asset_url }}) format("woff");
      }
    {% endstyle %}
     {{ 'slick.css' | asset_url | stylesheet_tag }}
    {{ 'slick-theme.css' | asset_url | stylesheet_tag }}
    {{ 'theme.css' | asset_url | stylesheet_tag }}
   

  </head>

  <body class="gradient">



    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    <script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
	  <script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    <script src="{{ 'slick.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'slick.min.js' | asset_url }}" defer="defer"></script>
  </body>
</html>
